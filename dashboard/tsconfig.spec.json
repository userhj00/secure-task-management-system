{
  "extends": "../tsconfig.base.json",
"compilerOptions": {
  "outDir": "./out-tsc/jest",
  "module": "nodenext",
  "target": "es2016",
  "types": ["jest", "node"],
  "moduleResolution": "nodenext"
},
  "files": ["src/test-setup.ts"],
  "include": [
    "jest.config.ts",
    "src/**/*.ts",
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "src/**/*.d.ts"
  ]
}
